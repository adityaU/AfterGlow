<h2 id="title">Welcome to Ember Gridstack</h2>

{{#grid-stack classNames='grid-stack' onChange=(action (mut test) true)}}
  {{#each items as |item|}}
    {{#grid-stack-item}}
      {{item}}
    {{/grid-stack-item}}
  {{/each}}
{{/grid-stack}}

<button onclick={{action 'changeItems'}}>+ New Item</button>

{{outlet}}
